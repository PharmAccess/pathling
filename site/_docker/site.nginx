server {
  listen 80;
  root /usr/share/nginx/html;
  error_page 404             /404.html;
  error_page 500 502 503 504 /50x.html;
  charset utf-8;
  gzip on;
  gzip_types text/css application/javascript application/json;
  gzip_comp_level 9;
  etag on;
  location /index.html {
    add_header Content-Security-Policy "default-src 'self'; font-src 'self' fonts.googleapis.com fonts.gstatic.com; style-src 'self' fonts.googleapis.com 'unsafe-inline'; img-src 'self' data:";
  }
}
